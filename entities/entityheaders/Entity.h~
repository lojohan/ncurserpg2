#ifndef ENTITY_H
#define ENTITY_H

#include <iostream>
#include <vector>

class Entity {
    private:
        std::vector<int> coords;
        
    public:
        Entity(int x, int y);
        
        void setX(int x);
        void setY(int y);
        void setCoords(int x, int y);
        
        int getX();
        int getY();
        vector<int> getCoords();
        
};

#endif
